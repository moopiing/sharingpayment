<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kaoneaw.moopiing.sharingpayment">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoadingActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AddMoneyActivity"></activity>
        <activity android:name=".ChooseActivity"></activity>
        <activity android:name=".InfoActivity"
            android:theme="@style/AppTheme.CustomTheme"></activity>
        <activity android:name=".JoinRoomActivity"></activity>
        <activity android:name=".LendMoneyActivity"></activity>
        <activity android:name=".LoginActivity"></activity>
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".NewRoomActivity"></activity>
        <activity android:name=".ResultActivity"></activity>
        <activity android:name=".SignUpActivity"></activity>

    </application>

</manifest>
