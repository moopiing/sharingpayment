<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kaoneaw.moopiing.sharingpayment">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.LoadingActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.AddMoneyActivity"></activity>
        <activity android:name=".Activities.ChooseActivity"></activity>
        <activity android:name=".Activities.InfoActivity"
            android:theme="@style/AppTheme.CustomTheme"></activity>
        <activity android:name=".Activities.JoinRoomActivity"></activity>
        <activity android:name=".Activities.LendMoneyActivity"></activity>
        <activity android:name=".Activities.LoginActivity"></activity>
        <activity android:name=".Activities.MainActivity"></activity>
        <activity android:name=".Activities.NewRoomActivity"></activity>
        <activity android:name=".Activities.ResultActivity"></activity>
        <activity android:name=".Activities.SignUpActivity"></activity>

    </application>

</manifest>
